<div class="container-fluid">
  <nav class="navbar navbar-expand-lg mb-0 p-0 fs-4 mx-lg-2">
    <%= link_to events_path class: "navbar-brand" do %>
      <% image_tag 'app_icon', style: "min-width: 85px"%>
    <% end %>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
            data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
            aria-label="Toggle navigation">
      <span class="navbar-toggler-icon fs-2 text-dark d-flex justify-content-center align-items-center">
        <i class="bi bi-distribute-vertical text-warning"></i>
      </span>
    </button>
    <% if current_user.present? %>
      <%= render 'shared/navbar/navbar_items' %>
    <% end %>     
  </nav>
</div>